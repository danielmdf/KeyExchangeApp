<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="at.buchberger.keyexchangeapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OwnKeyActivity" />
        //um in Dateien schreiben zu koennen
        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

        <activity android:name=".GeneratorActivity" />
        <activity android:name=".OwnPubKeyActivity" />
        <activity android:name=".AddPubKey" />
        <activity android:name=".AllPublicKeys" />
        <activity android:name=".DetailKey" />
        <activity android:name=".GeneratorVarActivity" />
    </application>

</manifest>